<h1>Branches</h1>

<%= link_to "New branch", new_branch_path %>

<br />
<br />

<table class="table table-striped">
  <% @branches.each do |branch| %>
    <tr id='<%= branch.id %>'>
      <td><%= branch.name %></td>
      <td><%= link_to "Edit", edit_branch_path(branch) %></td>
      <td><%= link_to "Delete", branch, method: :delete %></td>
    </tr>
  <% end %>
</table>
